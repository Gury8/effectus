; Effectus example
;
; Device Input/Output demonstration

PROC MAIN()
BYTE ARRAY TEXT_BUF(10)
CHAR KEY
CARD num

CLOSE(1)
OPEN(1,"S:",8,0)

PRINTE("PRINTING STRING VALUES TO CHANNEL 1")
PRINTE("ON S: DEVICE")
PUTE()
PRINTDE(1,"TEST1")
SCOPY(TEXT_BUF,"TEST2")
PRINTDE(1,TEXT_BUF)
PRINTD(1,"TEST3")
PRINTBD(1,10)
PUTE()
PRINTBDE(1,20)
PRINTID(1,2000)
PUTE()
PRINTIDE(1,10000)
PRINTCD(1,6230)
PUTE()
num=65334
PRINTCDE(1,num)
PUTE()
CLOSE(1)

KEY=GETD(7)

RETURN
