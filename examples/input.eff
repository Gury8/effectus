; Effectus example:
; String input demonstration
; using InputS procedure

BYTE ARRAY name(13), surname(21)

PROC Main()

Put(125)
PrintE("Effectus example:")
PrintE("String input demonstration")
PrintE("using InputS procedure")
PutE()

PrintE("What's your name?")
InputS(name)
PutE()
PrintE("What's your surname?")
InputS(surname)
PutE()
Print("Thank you, ")
Print(name)
Put(" ")
PrintE(surname)
PutE()

PutE()
PutE()
PrintE("End of program")

RETURN
