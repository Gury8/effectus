; Effectus example:
; Demonstration of using parameters
; with multiple operands

PROC Test()

BYTE x,y,r

x=80
y=10
r=60

Graphics(7)
Color=1
Plot(x,y)
DrawTo(x+r-10,y+r)
Color=2
DrawTo(x-r+10,y+r)
Color=3
Drawto(x,y)

x=80
y=14
r=54

Plot(x,y)
DrawTo(x+r-y,y+r)
Color=2
DrawTo(x-r+y,y+r)
Color=3
Drawto(x,y)

PrintE("Triangle")
        
RETURN
