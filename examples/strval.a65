A	= DATAORIGIN+$000B
RESULT	= DATAORIGIN+$000F

@VarData	= A
@VarDataSize	= 6

	rts						; ret
.endl

.local	STRTOINT_011C					; FUNCTION | ASSEMBLER | OVERLOAD

; ------------------------------------------------------------

; Generate Assignment for WORD / SMALLINT / SHORTREAL / POINTER
; as Pointer
	mva :STACKORIGIN,x S
	mva :STACKORIGIN+STACKWIDTH,x S+1
	dex						; sub bx, 1
	@move S #adr.S #33
	mwa #adr.S S

; ---------------------  ASM Block 059  ---------------------

	@StrToInt #adr.s

	mva edx Result
	mva edx+1 Result+1
	mva edx+2 Result+2
	mva edx+3 Result+3

@exit

