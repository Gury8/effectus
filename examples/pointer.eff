; Effectus example:
; POINTER variable demonstration

CARD POINTER ptr
INT i

PROC Main()

Put(125)
PrintE("Effectus example:")
PrintE("POINTER variable demonstration")
PutE()

i=70
PrintE("Current value of variable i: ")
PrintIE(I)

ptr=@I
PrintE("Memory address of variable i: ")
PrintCE(ptr)

PrintE(" ")

PrintE("New value of variable ptr and i: ")
ptr^=251
PrintC(ptr^)
Print(", ")
PrintIE(i)

PutE()
PutE()
PrintE("End of program")

RETURN
