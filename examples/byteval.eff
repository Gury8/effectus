; Variable manipulation demo

PROC MAIN()

BYTE N1=764, N2, N3=[12]

PUT(125)
N2=14
PRINT("Value of 764 is ") PRINTBE(N1)
PRINT("Value of N2 is ") PRINTBE(N2)
