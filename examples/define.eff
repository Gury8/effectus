; Effectus example:
; DEFINE statement demonstration

DEFINE Cls="PUT(125)"
DEFINE max="3"
DEFINE WriteLn="PrintBE"

PROC DEFINE_TEST()

DEFINE NewLine="PUTE()"

BYTE n

Cls
NewLine
NewLine
PrintE("DEFINE statement test")
NewLine
PutE()
PrintE("Cls, Newline")
NewLine 
PrintE("Count from 1 to max")
FOR n=1 to max DO
  WriteLn(n)
OD

RETURN
