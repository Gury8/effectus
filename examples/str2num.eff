; Effectus example
;
; String to number conversion demo
; using ValB, ValC and ValI functions 

PROC MAIN()

CHAR key
BYTE b INT i CARD c

B=VALB("120")
I=VALI("-3400")
C=VALC("65000")

PRINT("BYTE VALUE = ")
PRINTBE(B)

PRINT("INTEGER VALUE = ")
PRINTCE(I)

PRINT("WORD/CARD VALUE = ")
PRINTIE(C)

key=GETD(7)

RETURN
