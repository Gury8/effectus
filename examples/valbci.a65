.local	MAINPROC					; PROCEDURE

; optimize FAIL ('SYSUTILS.STRTOINT_0053', examples\valbci.pas), line = 19
	inx
	mva #$0D :STACKORIGIN,x
	mva #$20 :STACKORIGIN+STACKWIDTH,x
	jsr SYSUTILS.STRTOINT_0053
	mva :STACKORIGIN,x B
	dex

; optimize FAIL ('SYSUTILS.STRTOINT_0053', examples\valbci.pas), line = 20
	inx
	mva #$12 :STACKORIGIN,x
	mva #$20 :STACKORIGIN+STACKWIDTH,x
	jsr SYSUTILS.STRTOINT_0053
	mva :STACKORIGIN,x C
	mva :STACKORIGIN+STACKWIDTH,x C+1
	dex

; optimize FAIL ('SYSUTILS.STRTOINT_0053', examples\valbci.pas), line = 21
	inx
	mva #$19 :STACKORIGIN,x
	mva #$20 :STACKORIGIN+STACKWIDTH,x
	jsr SYSUTILS.STRTOINT_0053
	mva :STACKORIGIN,x I
	mva :STACKORIGIN+STACKWIDTH,x I+1
	mva :STACKORIGIN+STACKWIDTH*2,x I+2
	mva :STACKORIGIN+STACKWIDTH*3,x I+3
	dex

; optimize FAIL (100, examples\valbci.pas), line = 22
	inx
	mva B :STACKORIGIN,x
	jsr @printBYTE
	dex
	@printEOL

; optimize FAIL (100, examples\valbci.pas), line = 23
	inx
	mva C :STACKORIGIN,x
	mva C+1 :STACKORIGIN+STACKWIDTH,x
	jsr @printWORD
	dex
	@printEOL

; optimize FAIL (100, examples\valbci.pas), line = 24
	inx
	mva I :STACKORIGIN,x
	mva I+1 :STACKORIGIN+STACKWIDTH,x
	mva I+2 :STACKORIGIN+STACKWIDTH*2,x
	mva I+3 :STACKORIGIN+STACKWIDTH*3,x
	jsr @printINT
	dex
	@printEOL

; optimize FAIL ('CRT.READKEY', examples\valbci.pas), line = 25
	jsr CRT.READKEY
	mva :STACKORIGIN,x KEY
	dex

; ------------------------------------------------------------

KEY	= DATAORIGIN+$0032
B	= DATAORIGIN+$0033
C	= DATAORIGIN+$0034
I	= DATAORIGIN+$0036

